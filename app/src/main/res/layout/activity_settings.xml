<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="name.marinchenko.lorryvision.activities.main.SettingsActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/activity_settings_toolbar"
        style="@style/Toolbar" />

    <FrameLayout
        android:id="@+id/activity_settings_fragment_frame"
        android:layout_width="0dip"
        android:layout_height="0dip"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/activity_settings_toolbar" />

</android.support.constraint.ConstraintLayout>
